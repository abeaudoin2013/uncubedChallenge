<div>
	<%= form_for @list do |f| %>
		
		<p>
			<%= f.label :title %>
			<%= f.text_field :title %>
		</p>
		<p>
			<%= f.label :description %>
			<%= f.text_field :description %>
		</p>
		<p>
		  <%= f.label :progress %>
		  <%= f.number_field :progress %>
		</p>

		<p>
			<%= f.submit %>
		</p>

	<% end %> 

</div>
